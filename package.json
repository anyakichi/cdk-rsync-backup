{
  "name": "@anyakichi/cdk-rsync-backup",
  "description": "AWS CDK L3 construct for cloud backup system with rsync",
  "version": "0.4.1",
  "author": "INAJIMA Daisuke <inajima@gmail.com>",
  "scripts": {
    "build": "tsc",
    "format": "prettier --ignore-path .gitignore -w package.json lib",
    "prebuild": "node -p \"'export const LIB_VERSION = ' + JSON.stringify(require('./package.json').version) + ';'\" > lib/version.ts",
    "test": "jest",
    "watch": "tsc -w"
  },
  "types": "lib/index.d.ts",
  "main": "lib/index.js",
  "peerDependencies": {
    "aws-cdk-lib": "^2.0.0",
    "constructs": "^10.0.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/jest": "^29.5.1",
    "@types/node": "20.2.0",
    "@types/prettier": "2.7.2",
    "aws-cdk-lib": "^2.0.0",
    "constructs": "^10.0.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "prettier-plugin-fixpack": "^0.1.0",
    "ts-jest": "^29.1.0",
    "typescript": "~5.0.4"
  },
  "homepage": "https://github.com/anyakichi/cdk-rsync-backup",
  "repository": "https://github.com/anyakichi/cdk-rsync-backup",
